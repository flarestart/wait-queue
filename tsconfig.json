{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    // "sourceMap": true
  },
  "include": ["src/**/*.ts"]
}
